        <fullName>T_Signature_Dutch__c</fullName>
        <description>Dutch Signature based on gender used in Email Templates</description>
        <externalId>false</externalId>
        <formula>IF( TEXT( C_Gender__c ) = &quot;M&quot;,&quot;Uw Lampiris-medewerker&quot;,&quot;Uw Lampiris-medewerkster&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
