    <validationRules>
        <fullName>T_VF_No_Outbound_Emails_If_Case_Isnew</fullName>
        <active>true</active>
        <description>This validation rules forbids the user from sending emails if the status of the case is New</description>
        <errorConditionFormula>AND(
    ISNEW(),
    Incoming = FALSE,
    ISPICKVAL(Parent.Status, &quot;New&quot;),
    NOT(CONTAINS(Subject, &quot;Auto Response&quot;))
   )</errorConditionFormula>
        <errorMessage>You cannot send an email on a Case in Status New</errorMessage>
    </validationRules>
