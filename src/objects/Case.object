    <validationRules>
        <fullName>T_VF_Case_Is_Internal</fullName>
        <active>true</active>
        <description>This validation rule is used to tackle the creation of an Internal Case</description>
        <errorConditionFormula>AND(
    ISPICKVAL(Status,&quot;New&quot;),
    ISPICKVAL(Origin,&quot;Internal&quot;), 
    OR(
       ISBLANK(AccountId),
       ISBLANK(ContactId)
       ) 
    )</errorConditionFormula>
        <errorMessage>An Account and a Contact must always be filled in when creating an Internal Case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>T_VF_No_Close_Case_without_SQC</fullName>
        <active>true</active>
        <description>This Validation Rule is used to block a user to close a Case record without filling the fields &quot;Service&quot;, &quot;Question&quot; or &quot;Concern&quot; unless it is a SPAM case</description>
        <errorConditionFormula>AND(
    C_SPAM__c = FALSE,
    ISPICKVAL( Status ,&quot;Closed&quot;),
    ISBLANK(TEXT( C_Concern__c )),
NOT($Profile.Name = &quot;System Administrator&quot; ) 
    )</errorConditionFormula>
        <errorMessage>You cannot close a Case without Concern.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>T_VF_No_Escalation_If_Account_Ref_Conc</fullName>
        <active>true</active>
        <description>This Validation Rule is used to block a user to escalate a Case record if fields &quot;Account Reference&quot; or &quot;Concern&quot; is not filled.</description>
        <errorConditionFormula>AND(
    ISPICKVAL( Status , &quot;Escalated&quot;),
    OR(ISBLANK(TEXT(C_Concern__c)), 
       ISBLANK( C_Account_Reference__c ))
    )</errorConditionFormula>
        <errorMessage>You cannot escalate a case without a Concern and an Account Reference</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>T_VF_No_Update_On_Status_IF_Not_Owner</fullName>
        <active>true</active>
        <description>This validation rule forbids the user from changing the status of the case to working if he is not the owner of the case.</description>
        <errorConditionFormula>AND(
OwnerId &lt;&gt;  $User.Id,
NOT($Profile.Name = &quot;System Administrator&quot; ) ,
ISPICKVAL(Status, &quot;Working&quot;),
ISCHANGED(Status)
)</errorConditionFormula>
        <errorMessage>You must first take ownership of the case before updating the Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>T_VF_No_Update_Possible_Without_Owner</fullName>
        <active>true</active>
        <description>This rule prevents an agent from editing a case if the case is not first removed from the queue.</description>
        <errorConditionFormula>ISPICKVAL(Status, &quot;New&quot;)
&amp;&amp;
NOT($Profile.Name = &quot;System Administrator&quot; )
&amp;&amp;
NOT($Profile.Name = &quot;Supervisor&quot; )
&amp;&amp;
NOT(ISCHANGED(OwnerId))
&amp;&amp;
OwnerId &lt;&gt;  $User.Id
&amp;&amp;
NOT (ISCHANGED ( C_Detected_Language__c))</errorConditionFormula>
        <errorMessage>You cannot edit this record before first taking ownership</errorMessage>
    </validationRules>
